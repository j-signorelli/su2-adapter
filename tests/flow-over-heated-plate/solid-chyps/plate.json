{
	"SimConfig":{
		"TemperatureSolver":{
			"class": "ThermalConduction"
		},
		"SolidSolver":{
			"class": "ConstantSolid"
		}
	},
	
	"SimulationInitializer": {
		"out_data": "Init",
		"FieldInitialization": {
			// Initialize constant temperature field of 310
			"temperature": {
				"dimensionality": "scalar",
				"field_type": "grid_function",
				"initializer": "constant",
				"Arguments": {
					"value": 310.0
				}
			},
			// Initialize constant thermal conductivity field of 100
			"thermal_conductivity": {
				"dimensionality": "scalar",
				"field_type": "grid_function",
				"initializer": "constant",
				"Arguments": {
					"value": 100
				}
			},
			// Initialize constant density field of 0.1
			"solid_tracker/solids/Solid/density": {
				"dimensionality": "scalar",
				"field_type": "grid_function",
				"initializer": "constant",
				"Arguments": {
					"value": 0.1
				}
			},
			
			// Initialize constant cp field of 1000
			"solid_tracker/solids/Solid/cp": {
				"dimensionality": "scalar",
				"field_type": "grid_function",
				"initializer": "constant",
				"Arguments": {
					"value": 1000.0
				}
			},
			// Initialize constant volume fraction field of 1
			"solid_tracker/solids/Solid/volume_fraction": {
				"dimensionality": "scalar",
				"field_type": "grid_function",
				"initializer": "constant",
				"Arguments": {
					"value": 1.0
				}
			}

		}
	},
	
	"Simulation": {
		"in_data": "Init",
		"out_data": "Out",
		"precice_solver_name": "Solid",
		"precice_config": "../precice-config.xml",
		"ProgressionTracker": {
			"end_time": 1.0,
			"TimeStepControl":{
				"type": "constant",
				"time_step": 0.01
			}
		},
		
		"Visualization":{
			"frequency": 0.2,
			"output_format": "paraview"
		},
		
		"Checkpoint":{
			"output_name": "Solution",
			"frequency": 0.2
		}
	},
	
	"FiniteElementDefinition":{
		"order": 1,
		"bc_order": 1
	},
	
	"BoundaryConditions":{
		"temperature": {
			"DIRICHLET": {
				"1": {
					"value":{
						"type": "constant",
						"value": 310.0
					}
				}
			},
			"HOMOGENEOUS_NEUMANN": {
				"2": {}
			},
			"NEUMANN":{
				"3": {
					"value": {
						"type": "precice",
						"precice_name": "Heat-Flux"
					}
				}
			},
			"HOMOGENEOUS_NEUMANN": {
				"4": {}
			}
		}
	},
	"Mesh":{
		"mesh": "generate",
		"serial_refine": 0,
		"parallel_refine": 0,
		"Generator":{
			"engine": "uniform_cuboid",
			"lower_point": [0,-0.25],
			"upper_point": [1,0],
			"number_of_elements": [20, 5]
		},
		"precice_mesh_names": ["","","Solid-Mesh"]
	},
	"SolidSolver": {
		"Materials": {
		  "Solid": {}
		},
		"ThermalConductivity": {
		  "dimensionality": "scalar"
		}
	},
	"TemperatureSolver": {},

	"PreciceWrite": {
		"Temperature":{
			"3": {
				"field_name": "temperature"
			}
		}
	}
}
		
		
		